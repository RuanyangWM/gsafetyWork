public class PrintNum {

        public static void main(String[] args) {
            String str[]={"ruanyang","good","boy"};
            int lengh = str.length;
            if (lengh == 0) {
                System.out.println("无参数");
            } else {
                for (int i = 1; i <= str.length; i++) {
                    System.out.println(i);
                }
            }
        }
}
